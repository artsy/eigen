{
  "extends": [
    "tslint:recommended",
    "react"
  ],
  "rules": {
    "trailing-comma": [
      false
    ],
    "arrow-parens": false,
    "semicolon": [
      true,
      "never",
      "ignore-interfaces",
      "ignore-bound-class-methods"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "max-classes-per-file": [
      false
    ],
    "member-access": [
      false,
      "check-accessor",
      "check-constructor"
    ]
  }
}
